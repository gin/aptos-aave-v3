    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-tsdoc": "^0.4.0",
    "jest": "^29.7.0",
    "node-ts": "^8.0.2",
    "npm-run-all": "^4.1.5",
    "ora": "^8.2.0",
    "prettier": "^3.5.3",
    "prettier-eslint": "^16.4.1",
